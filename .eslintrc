{
  "env": {
    "browser": true
  },
  "extends": ["airbnb-base", "prettier"],
  "overrides": [
    {
      "files": ["./**/*.svelte"],
      "processor": "svelte3/svelte3"
    }
  ],
  "parserOptions": {
    "ecmaVersion": 2020,
    "sourceType": "module"
  },
  "plugins": ["svelte3"],
  "root": true,
  "rules": {
    "import/no-mutable-exports": "off",
    "import/prefer-default-export": "off",
    "no-console": "off"
  }
}
